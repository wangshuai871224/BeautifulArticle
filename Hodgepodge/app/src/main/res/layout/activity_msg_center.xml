<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/m_write">

    <com.example.dllo.hodgepodge.mine.msgcenter.WheelView
        android:id="@+id/activity_msg_center_lucky_wheel"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_centerInParent="true"
        android:padding="@dimen/m_margin_thirty"/>

    <ImageButton
        android:id="@+id/activity_msg_center_start"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:background="@mipmap/start"/>
</RelativeLayout>